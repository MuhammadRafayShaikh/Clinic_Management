<h2>Machine Stock Report</h2>
<h2>Machine Stock Report</h2>
<h2 class="text-center">Seminar Report</h2>

<table class="table table-striped table-hover table-bordered">
    <thead class="table-dark">
        <tr>
            <th>#</th>
            <th>Seminar Name</th>
            <th>Price</th>
            <th>Total Bookings</th>
            <th>Total Earnings</th>
        </tr>
    </thead>
    <tbody>
        @if (ViewBag.report != null)
        {
            int count = 1;
            @foreach (var report in ViewBag.report)
            {
                <tr>
                    <td>@count</td>
                    <td>@report.SeminarName</td>
                    <td>@report.Price</td>
                    <td>@report.TotalBookings</td>
                    <td>@report.TotalEarnings</td>
                </tr>
                count++;
            }
        }
        else
        {
            <tr>
                <td colspan="9" class="text-center">No data available</td>
            </tr>
        }
    </tbody>
</table>
